{
    "name": "chatwith",
    "version": "0.1.2",
    "license": "MIT",
    "bin": "dist/index.js",
    "type": "module",
    "engines": {
        "node": ">=16"
    },
    "scripts": {
        "dev": "nodemon -e ts,tsx,json --exec 'node --experimental-specifier-resolution=node --experimental-loader=extensionless --loader ts-node/esm' src/index.tsx",
        "build": "tsc && chmod +x dist/index.js",
        "start": "npm run build && node --experimental-loader=extensionless dist/index.js",
        "pretest": "npm run build"
    },
    "dependencies": {
        "env-paths": "^3.0.0",
        "extensionless": "^1.4.5",
        "ink": "^4.0.0",
        "ink-text-input": "^5.0.0",
        "make-dir": "^3.1.0",
        "meow": "^9.0.0",
        "openai": "^3.2.1",
        "react": "^18.0.0",
        "zod": "^3.21.4"
    },
    "devDependencies": {
        "@types/node": "^18.15.11",
        "@types/react": "^18.0.31",
        "eslint-plugin-react": "^7.32.2",
        "eslint-plugin-react-hooks": "^4.6.0",
        "ink-testing-library": "^3.0.0",
        "nodemon": "^2.0.22",
        "ts-node-dev": "^2.0.0",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.0.3"
    },
    "extensionless": {
        "lookFor": [
            "js",
            "jsx"
        ]
    }
}
